$('#photos').html("<%= j render 'photos_list' %>")
<% if @bouncehouse.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@bouncehouse.active && !@bouncehouse.price.blank? && !@bouncehouse.listing_name.blank? && !@bouncehouse.photos.blank? && !@bouncehouse.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>