$('#photos').html("<%= j render 'photos_list' %>")
<% if @.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@.active && !@.price.blank? && !@.listing_name.blank? && !@.photos.blank? && !@.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>