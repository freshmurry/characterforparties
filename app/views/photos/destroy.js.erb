$('#photos').html("<%= j render 'photos_list' %>")
<% if @pool.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@pool.active && !@pool.price.blank? && !@pool.listing_name.blank? && !@pool.photos.blank? && !@pool.location_type.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>